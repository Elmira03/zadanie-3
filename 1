#include <iostream>
#include <math.h>
using namespace std;
int lala(int n) {
	int sum;
	sum = 0;
	while (n>0)
	{
		n = n / 10;
		sum = sum + 1;
	}
	return sum;
}
int main() {
	int n;
	int v;
	cin >> n;
	v=lala(n);
	while (v > 0) {
		int a;int b;
		a = 10;
		v = v - 1;
		cout <<( n / int(pow(a, v)))<<endl;
		n = n %int( pow(a, v));
	}
}
